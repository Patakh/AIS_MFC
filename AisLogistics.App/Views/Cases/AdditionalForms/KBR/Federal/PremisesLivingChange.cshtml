        Model.FamilyMembers = new[] { new AisLogistics.App.Models.AdditionalForms.KBR.Federal.PremisesLivingChangeModel.FamilyMember() };

    if (Model.AppliedDocumentList.Length == 0)
        Model.AppliedDocumentList = new[] { new AisLogistics.App.Models.AdditionalForms.KBR.Federal.PremisesLivingChangeModel.AppliedDocument() };
}
