                                                        <label asp-for="LegalEntityAddress" class="form-label">Адрес</label>
                                                        <input asp-for="LegalEntityAddress" class="form-control" type="text">
                                                    </div>
                                                    <div class="col-4">
                                                        <label asp-for="LegalEntityPhone" class="form-label">Телефон</label>
                                                        <input asp-for="LegalEntityPhone" class="form-control pickdate" type="text">
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mb-3">
                        <div class="col-12" checkBlock>
                            <div class="form-check">
                                <input asp-for="ApplicantTypeId" id="ApplicantTypeId4" class="form-check-input" type="radio" value="4">
                                <label for="ApplicantTypeId4" class="form-check-label">Представитель юридического лица</label>
                            </div>
                            <div class="row mb-3 justify-content-end" checkBlockBody>
                                <div class="row mb-3">
                                    <div class="col-12">
                                        <div class="row mb-3 justify-content-end">
                                            <div class="col-11 mb-2">
                                                <div class="row mb-3">
                                                    <div class="col-12">
                                                        <label asp-for="LegalEntityAgentFullName" class="form-label">Ф.И.О.</label>
                                                        <input asp-for="LegalEntityAgentFullName" class="form-control" type="text">
                                                    </div>
                                                    <h6 class="my-2">Реквизиты доверенности</h6>
                                                    <div class="row mb-3">
                                                        <div class="col-4">
                                                            <label asp-for="LegalEntityAgentDocumentSeries" class="form-label">Серия</label>
                                                            <input asp-for="LegalEntityAgentDocumentSeries" class="form-control" type="text">
                                                        </div>
                                                        <div class="col-4">
                                                            <label asp-for="LegalEntityAgentDocumentNumber" class="form-label">Номер</label>
                                                            <input asp-for="LegalEntityAgentDocumentNumber" class="form-control" type="text">
                                                        </div>
                                                        <div class="col-4">
                                                            <label asp-for="LegalEntityAgentDocumentGivenDate" class="form-label">Дата выдачи</label>
                                                            <input asp-for="LegalEntityAgentDocumentGivenDate" class="form-control pickdate" type="text">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <h5 class="my-2">Место нахождения помещения</h5>
            <div class="row mb-3">
                <div class="col-12">
                    <div class="row mb-3">
                        <div class="col-12">
                            <label asp-for="SubjectRussianFederationName" class="form-label">Наименование субъекта Российской Федерации</label>
                            <input asp-for="SubjectRussianFederationName" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-12">
                            <label asp-for="MunicipalityName" class="form-label">
                                Наименование муниципального района, городского округа или внутригородской территории (для городов федерального значения)
                                в составе субъекта Российской Федерации
                            </label>
                            <input asp-for="MunicipalityName" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-12">
                            <label asp-for="SettlementName" class="form-label">Наименование поселения</label>
                            <input asp-for="SettlementName" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-12">
                            <label asp-for="Street" class="form-label">Улица</label>
                            <input asp-for="Street" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-4">
                            <label asp-for="House" class="form-label">Дом</label>
                            <input asp-for="House" class="form-control" type="text">
                        </div>
                        <div class="col-4">
                            <label asp-for="Housing" class="form-label">Корпус</label>
                            <input asp-for="Housing" class="form-control" type="text">
                        </div>
                        <div class="col-4">
                            <label asp-for="Building" class="form-label">Строение</label>
                            <input asp-for="Building" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-4">
                            <label asp-for="Entrance" class="form-label">Подъезд</label>
                            <input asp-for="Entrance" class="form-control" type="text">
                        </div>
                        <div class="col-4">
                            <label asp-for="Floor" class="form-label">Этаж</label>
                            <input asp-for="Floor" class="form-control" type="text">
                        </div>
                        <div class="col-4">
                            <label asp-for="Flat" class="form-label">Квартира (комната)</label>
                            <input asp-for="Flat" class="form-control" type="text">
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-3">
                <div class="col-12">
                    <label asp-for="PremisesOwner" class="form-label">Собственник(и) жилого помещения</label>
                    <input asp-for="PremisesOwner" class="form-control" type="text">
                </div>
            </div>

            <h5 class="my-2">Прошу разрешить</h5>
            <div class="row mb-3">
                <div class="col-12">
                    <div class="form-check">
                        <input asp-for="ChangingTypeId" id="ChangingTypeId1" class="form-check-input" type="radio" value="1">
                        <label for="ChangingTypeId1" class="form-check-label">жилое помещение в нежилое</label>
                    </div>
                    <div class="form-check">
                        <input asp-for="ChangingTypeId" id="ChangingTypeId2" class="form-check-input" type="radio" value="2">
                        <label for="ChangingTypeId2" class="form-check-label">нежилое помещение в жилое</label>
                    </div>
                </div>
            </div>

            <h5 class="my-2">Перевод на основании</h5>
            <div class="row mb-3">
                <div class="col-12">
                    <div class="form-check">
                        <input asp-for="ChangingReasonTypeId" id="ChangingReasonTypeId1" class="form-check-input" type="radio" value="1">
                        <label for="ChangingReasonTypeId1" class="form-check-label">право собственности</label>
                    </div>
                    <div class="form-check">
                        <input asp-for="ChangingReasonTypeId" id="ChangingReasonTypeId2" class="form-check-input" type="radio" value="2">
                        <label for="ChangingReasonTypeId2" class="form-check-label">договор найма</label>
                    </div>
                </div>
            </div>

            <h5 class="my-2">Производство ремонтно-строительных работ</h5>
            <div class="row mb-3">
                <div class="col-12">
                    <div class="row mb-3">
                        <div class="col-12">
                            <div class="row mb-3">
                                <h6 class="my-2">Диапазон дат</h6>
                                <div class="col-6">
                                    <label asp-for="ReconstructionWorkStartDate" class="form-label">Дата начала</label>
                                    <input asp-for="ReconstructionWorkStartDate" class="form-control pickdate" type="text">
                                </div>
                                <div class="col-6">
                                    <label asp-for="ReconstructionWorkEndDate" class="form-label">Дата окончания</label>
                                    <input asp-for="ReconstructionWorkEndDate" class="form-control pickdate" type="text">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-12">
                            <div class="row mb-3">
                                <h6 class="my-2">Диапазон часов</h6>
                                <div class="col-6">
                                    <label asp-for="ReconstructionWorkStartHour" class="form-label">Начало</label>
                                    <input asp-for="ReconstructionWorkStartHour" class="form-control" type="number">
                                </div>
                                <div class="col-6">
                                    <label asp-for="ReconstructionWorkEndHour" class="form-label">Конец</label>
                                    <input asp-for="ReconstructionWorkEndHour" class="form-control" type="number">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-12">
                            <label asp-for="ReconstructionWorkDaysName" class="form-label">Дни недели</label>
                            <input asp-for="ReconstructionWorkDaysName" class="form-control" type="text">
                        </div>
                    </div>
                </div>
            </div>
            
            <h5 class="my-2">
                Согласие   на  перевод  помещения  получено  от  совместно  проживающих
                совершеннолетних членов семьи нанимателя жилого помещения по договору социального найма
            </h5>
            <div class="row mb-3">
                <div class="col-6">
                    <label asp-for="ReconstructionSocialContractDate" class="form-label">Дата договора социального найма</label>
                    <input asp-for="ReconstructionSocialContractDate" class="form-control pickdate" type="text">
                </div>
                <div class="col-6">
                    <label asp-for="ReconstructionSocialContractNumber" class="form-label">Номер договора социального найма</label>
                    <input asp-for="ReconstructionSocialContractNumber" class="form-control" type="text">
                </div>
            </div>

            <div id="familyMemberList" data-clone-list>
                @for (var i = 0; i < Model.FamilyMembers.Length; i++)
                {
                    <div class="g-3" id="clone@(i)" data-id="@i" data-clone>
                        <hr />
                        <h6 class="my-2 title">Член семьи @(i + 1)</h6>
                        <div class="row mb-1 align-items-center">
                            <div class="col-10">
                                <label asp-for="FamilyMembers[i].FullName" class="form-label">Ф.И.О.</label>
                                <input asp-for="FamilyMembers[i].FullName" class="form-control" type="text">
                            </div>
                            <div class="col-1 mt-2">
                                <button type="button" class="btn btn-danger" onclick="blockRemove(this)"><i class="bi bi-trash"></i></button>
                            </div>
                        </div>
                        <div class="row mb-1">
                            <div class="col-12">
                                <h7>Документ, удостоверяющий личность</h7>
                                <div class="row mb-1">
                                    <div class="col-4">
                                        <label asp-for="FamilyMembers[i].DocumentSeries" class="form-label">Серия</label>
                                        <input asp-for="FamilyMembers[i].DocumentSeries" class="form-control" type="text">
                                    </div>
                                    <div class="col-4">
                                        <label asp-for="FamilyMembers[i].DocumentNumber" class="form-label">Номер</label>
                                        <input asp-for="FamilyMembers[i].DocumentNumber" class="form-control" type="text">
                                    </div>
                                    <div class="col-4">
                                        <label asp-for="FamilyMembers[i].DocumentGivenDate" class="form-label">Дата выдачи</label>
                                        <input asp-for="FamilyMembers[i].DocumentGivenDate" class="form-control pickdate" type="text">
                                    </div>
                                </div>
                                <div class="row mb-1">
                                    <div class="col-4">
                                        <label asp-for="FamilyMembers[i].DocumentGivenBy" class="form-label">Кем выдан</label>
                                        <input asp-for="FamilyMembers[i].DocumentGivenBy" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <div class="row justify-content-center">
                <div class="col-2">
                    <button type="button" class="btn btn-primary" id="addFamilyMember"><i class="bi bi-plus"></i></button>
                </div>
            </div>
            <hr />

            <div id="appliedDocumentList" data-clone-list>
                @for (var i = 0; i < Model.AppliedDocumentList.Length; i++)
                {
                    <div class="g-3" id="clone@(i)" data-id="@i" data-clone>
                        <hr />
                        <h6 class="my-2 title">Документ @(i + 1)</h6>
                        <div class="row mb-1 align-items-center">
                            <div class="col-6">
                                <div class="form-check">
                                    <input asp-for="AppliedDocumentList[i].TypeId" id="TypeId@(i + 1)1" class="form-check-input" type="radio" value="1">
                                    <label for="TypeId@(i + 1)1" class="form-check-label">подлинник</label>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-check">
                                    <input asp-for="AppliedDocumentList[i]" id="TypeId@(i + 1)2" class="form-check-input" type="radio" value="2">
                                    <label for="TypeId@(i + 1)2" class="form-check-label">нотариально заверенная копия</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-1 align-items-center">
                            <div class="col-10">
                                <label asp-for="AppliedDocumentList[i].Name" class="form-label">Наименование</label>
                                <input asp-for="AppliedDocumentList[i].Name" class="form-control" type="text">
                            </div>
                            <div class="col-1 mt-2">
                                <button type="button" class="btn btn-danger" onclick="blockRemove(this)"><i class="bi bi-trash"></i></button>
                            </div>
                        </div>
                        <div class="row mb-1">
                            <div class="col-4">
                                <label asp-for="AppliedDocumentList[i].Number" class="form-label">Номер</label>
                                <input asp-for="AppliedDocumentList[i].Number" class="form-control" type="text" />
                            </div>
                            <div class="col-4">
                                <label asp-for="AppliedDocumentList[i].GivenDate" class="form-label">Дата выдачи</label>
                                <input asp-for="AppliedDocumentList[i].GivenDate" class="form-control pickdate" type="text" />
                            </div>
                            <div class="col-4">
                                <label asp-for="AppliedDocumentList[i].PagesAmount" class="form-label">Кол-во листов</label>
                                <input asp-for="AppliedDocumentList[i].PagesAmount" class="form-control" type="number" min="0" />
                            </div>
                        </div>
                        <div class="row mb-1">
                            <div class="col-12">
                                <label asp-for="AppliedDocumentList[i].GivenBy" class="form-label">Кем выдан</label>
                                <input asp-for="AppliedDocumentList[i].GivenBy" class="form-control" type="text" />
                            </div>
                        </div>
                    </div>
                }
            </div>
            <div class="row justify-content-center">
                <div class="col-2">
                    <button type="button" class="btn btn-primary" id="addAppliedDocument"><i class="bi bi-plus"></i></button>
                </div>
            </div>
            <hr />
        </div>
    </div>
</div>

