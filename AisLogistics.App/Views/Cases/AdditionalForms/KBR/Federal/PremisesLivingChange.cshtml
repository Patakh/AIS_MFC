                                                <div class="row mb-3">
                                                    <div class="col-12">
                                                        <label asp-for="ApplicantAgentFullName" class="form-label">Ф.И.О.</label>
                                                        <input asp-for="ApplicantAgentFullName" class="form-control" type="text">
                                                    </div>
                                                    <h6 class="my-2">Реквизиты доверенности</h6>
