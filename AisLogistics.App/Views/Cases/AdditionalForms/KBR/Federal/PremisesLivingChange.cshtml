                                    <div class="col-12">
                                        <div class="row mb-3 justify-content-end">
                                            <div class="col-11 mb-2">
                                                <div class="row mb-3">
                                                    <div class="col-12">
                                                        <label asp-for="LegalEntityFullName" class="form-label">Наименование</label>
                                                        <input asp-for="LegalEntityFullName" class="form-control" type="text">
                                                    </div>
                                                </div>
                                                <div class="row mb-3">
                                                    <div class="col-4">
                                                        <label asp-for="LegalEntityForm" class="form-label">Организационно-правовая форма</label>
                                                        <input asp-for="LegalEntityForm" class="form-control" type="text">
                                                    </div>
