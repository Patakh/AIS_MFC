                                                            <input asp-for="ApplicantAgentDocumentSeries" class="form-control" type="text">
                                                        </div>
                                                        <div class="col-4">
                                                            <label asp-for="ApplicantAgentDocumentNumber" class="form-label">Номер</label>
                                                            <input asp-for="ApplicantAgentDocumentNumber" class="form-control" type="text">
