<script>
    var additionalForm = $('#additional-data-content');
    var originalBlock = $('#originalBlock');
    var btnNext = $('.btn-next');

    var applicant = {
        f: $("#Applicant_F"),
        i: $("#Applicant_I"),
        o: $("#Applicant_O"),
        birthDate: $("#Applicant_BirthDate"),
        birthPlace: $("#Applicant_AddressBirth"),
        SNILS: $("#Applicant_SNILS"),
        INN: $("#Applicant_INN"),
        docSeries: $("#Applicant_DocSeries"),
        docNumber: $("#Applicant_DocNumber"),
        docIssueDate: $("#Applicant_DocIssueDate"),
        docIssuer: $("#Applicant_DocIssuer"),
        docDevision: $("#Applicant_DocDevision"),
    };

    $(function () {

        if (btnNext.length > 0) {
            btnNext.on('click', function appContent() {
                applicant.f.val($("#customer_LastName").val());
                applicant.i.val($("#customer_FirstName").val());
                applicant.o.val($("#customer_SecondName").val());
                applicant.birthDate.val($("#customer_DocumentBirthDate").val());
                applicant.birthPlace.val($("#customer_DocumentBirthPlace").val());
                applicant.INN.val($("#customer_CustomerInn").val());
                applicant.SNILS.val($("#customer_CustomerSnils").val());
                applicant.docSeries.val($("#customer_DocumentSerial").val());
                applicant.docNumber.val($("#customer_DocumentNumber").val());
                applicant.docIssueDate.val($("#customer_DocumentIssueDate").val());
                applicant.docIssuer.val($("#customer_DocumentIssueBody").val());
                applicant.docDevision.val($("#customer_DocumentCode").val());
                switch ($('[name="customer.CustomerGender"]').val()) {
                    case '1':
                        $('#Applicant_Gender1').prop('checked', true);
                        break;
                    case '2':
                        $('#Applicant_Gender2').prop('checked', true);
                        break;
                }
            });
        };

        originalBlock.find('[data-original]').each(function (_, item) {
            renameBlock($(item), 100, true);
            visibilityBlock($(item), true, true);
        });

        additionalForm.find('[data-disabled]').each(function () {
            visibilityBlock($(this), true, true);
        });
         
        $.post(`/SmevBase/SmevGetDictionary`,
            {
                type: 'FnsRegionCodes'
            },
            function (dictionary) {
                dictionary.dictionary.forEach(function (item) {
                    $('[name$="Region"]').append(new Option(item.value, item.key));
                });
            });

        additionalForm.find('#addFamilyDocument').on('click', () => addForm('FamilyDocument', 'FamilyList'));
        additionalForm.find('#addAppliedDocument').on('click', () => addForm('appliedDocumentList', 'appliedDocument'));
        additionalForm.find("select.select2-search").each(function () {
            $(this).select2({
                //dropdownParent: $("#mainModal"),
            });
        });

        additionalForm.find("select.select2-nosearch").each(function () {
            $(this).select2({
                minimumResultsForSearch: Infinity,
            });
        });

        let options = { clearIncomplete: true, showMaskOnHover: false };
        additionalForm.find('[id$="_DocSeries"]').inputmask("9999", options);
        additionalForm.find('[id$="_DocNumber"]').inputmask("999999", options);
        additionalForm.find('[id$="INN"]').inputmask("9{12}", options);
        additionalForm.find('[id$="OGRN"]').inputmask("9{13}", options);
        additionalForm.find('[id$="INN_S"]').inputmask("9{10}", options);
        additionalForm.find('[id$="OGRN_S"]').inputmask("9{13}", options);
        additionalForm.find('[id$="INN_W"]').inputmask("9{12}", options);
        additionalForm.find('[id$="OGRN_W"]').inputmask("9{13}", options);
        additionalForm.find('[id$="_DocDevision"]').inputmask("999-999", options);
        additionalForm.find('[id$="BirthDocSeries"]').inputmask("", { regex: "^[XIVLMC]{1,4}-[А-ЯЁ]{2}$", clearIncomplete: true, showMaskOnHover: false });
        additionalForm.find('[id$="SNILS"]').inputmask("999-999-999 99", options);

        additionalForm.find(".pickdate").datepicker({
            language: "ru",
            autoclose: true,
        }).inputmask("99.99.9999", options);

        function addForm(cloneListId, originalId) { //Динамическое добавление Принимает (Куда Откуда)
            let $cloneList = additionalForm.find(`#${cloneListId}`);
            let $original = originalBlock.find(`#${originalId}`);
            let $cloneBlock = $original.clone(true);
            let indexId = $cloneList.find('[data-clone]').length;
            renameBlock($cloneBlock, indexId);
            blockInit($cloneBlock);
            $cloneList.find("select").each(function () {
                $(this).select2('destroy');
            });
            $cloneList.append($cloneBlock);
            select2Init($cloneList);
        }

        function blockInit($block) {
            visibilityBlock($block, false, false);

            $block.find("input[type='text']").each(function () {
                $(this).val("");
            });

        }
    });

    function select2Init($block) {

        $.post(`/SmevBase/SmevGetDictionary`,
            {
                type: 'FnsRegionCodes'
            },
            function (dictionary) {
                dictionary.dictionary.forEach(function (item) {
                    $block.find('[name$="Region"]').append(new Option(item.value, item.key));
                });
            });
        $block.find("select.select2-nosearch").each(function () {
            $(this).select2({
                minimumResultsForSearch: Infinity
            });
        });

        $block.find(".pickdate").datepicker({
            language: "ru",
            autoclose: true,
        }).inputmask("99.99.9999", { clearIncomplete: true, showMaskOnHover: false });

        $block.find("select.select2-search").each(function () {
            $(this).select2({
            });
        });
        let options = { clearIncomplete: true, showMaskOnHover: false };
        $block.find('[id$="_DocSeries"]').inputmask("9999", options);
        $block.find('[id$="_DocNumber"]').inputmask("999999", options);
        $block.find('[id$="INN"]').inputmask("9{12}", options);
        $block.find('[id$="OGRN"]').inputmask("9{13}", options);
        $block.find('[id$="INN_S"]').inputmask("9{10}", options);
        $block.find('[id$="OGRN_S"]').inputmask("9{13}", options);
        $block.find('[id$="INN_W"]').inputmask("9{12}", options);
        $block.find('[id$="OGRN_W"]').inputmask("9{13}", options);
        $block.find('[id$="_DocDevision"]').inputmask("999-999", options);
        $block.find('[id$="BirthDocSeries"]').inputmask("", { regex: "^[XIVLMC]{1,4}-[А-ЯЁ]{2}$", clearIncomplete: true, showMaskOnHover: false });
        $block.find('[id$="SNILS"]').inputmask("999-999-999 99", options);
    }

    function renameBlock($cloneBlock, indexId, initOriginal = false) {
        if (!initOriginal) {
            $cloneBlock.find('.title').text($cloneBlock.find('.title').text().replace(/\d+/, indexId + 1));
            $cloneBlock.attr("id", `${$cloneBlock.attr("id")}${indexId}`);
            $cloneBlock.attr('data-id', indexId);
            $cloneBlock.attr('data-clone', '');
            $cloneBlock.removeAttr('data-original');
        }

        $cloneBlock.find('input, select, div').each(function () {

            $(this).attr('id', $(this).attr('id')?.replace(/\d+/, indexId));

            if ($(this).is('[name]')) {
                $(this).attr('name', $(this).attr('name').replace(/\d+/, indexId));
            }
        });

        $cloneBlock.find('label').each(function () {
            $(this).attr('for', $(this).attr('for').replace(/\d+/, indexId));
        });
    }

    function blockRemove(el) {
        let $cloneList = $(el).closest('[data-clone-list]');
        el.closest('[data-clone]').remove();
        $cloneList.find('[data-clone]').each(function (index, item) {
            renameBlock($(item), index);
        });
    }

    function visibilityBlock($block, hide, isDisabled) {
        switch (hide) {
            case true:
                $block.hide();
                break;
            case false:
                $block.show();
                break;
        }

        switch (isDisabled) {
            case true:
                $block.find('input, select').each(function () {
                    $(this).prop('disabled', true);
                });
                break;
            case false:
                $block.find('input, select').each(function () {
                    $(this).prop('disabled', false);
                });
                $block.find('[data-disabled]').each(function () {
                    visibilityBlock($(this), true, true);
                });
                break;
        }
    }
</script>
